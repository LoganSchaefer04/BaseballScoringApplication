<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane layoutX="171.0" layoutY="103.0" prefHeight="305.0" prefWidth="234.0">
         <content>
            <VBox fx:id="awayTeamBattingOrder" prefHeight="303.0" prefWidth="201.0" />
