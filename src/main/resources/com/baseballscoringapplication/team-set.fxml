      </ScrollPane>
      <Label alignment="BASELINE_CENTER" layoutX="3.0" prefHeight="68.0" prefWidth="315.0" text="Team Select Screen">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <ComboBox fx:id="awayTeamDropdown" layoutX="14.0" layoutY="103.0" onAction="#setGameAwayTeam" prefWidth="150.0" promptText="Select Away Team" />
      <ComboBox fx:id="homeTeamDropdown" layoutX="14.0" layoutY="456.0" onAction="#setGameHomeTeam" prefWidth="150.0" promptText="Select Home Team" />
      <Button fx:id="startGameButton" layoutX="864.0" layoutY="709.0" lineSpacing="1.0" mnemonicParsing="false" onAction="#startGame" prefHeight="85.0" prefWidth="130.0" text="START&#10;GAME!" textAlignment="CENTER">
